        <serviceTask id="sid-FDDF4E8E-EBFC-445C-B647-68CEC6E1315A" name="Generate Acknowledgement Letter" activiti:expression="#{acknowledgementDocumentService.generateAndUpload(OBJECT_TYPE, OBJECT_ID)}">
