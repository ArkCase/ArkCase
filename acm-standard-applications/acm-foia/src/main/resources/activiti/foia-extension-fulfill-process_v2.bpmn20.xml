    <serviceTask id="sid-E0905ADB-9340-49AC-ADA5-3AD0B417D585" name="Set FOIA Request status" activiti:expression="#{changeObjectStatusService.change(OBJECT_ID, OBJECT_TYPE, 'In Fulfillment')}">
