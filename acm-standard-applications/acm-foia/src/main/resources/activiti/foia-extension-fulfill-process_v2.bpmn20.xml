    <serviceTask id="sid-71F59272-D1D8-4F45-925A-8123AFA15CB0" name="Send Mail To Notification Group" activiti:expression="#{foiaExecutiveGroupNotificationService.sendFulfillEmailNotification(OBJECT_ID)}">
