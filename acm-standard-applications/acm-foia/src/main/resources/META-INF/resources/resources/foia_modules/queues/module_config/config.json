        {
            "menuId": "leftnav",
            "menuItemURL": "queues",
            "menuItemTitle": "queues.menus.leftnav.item",
            "moduleId": "queue",
            "iconClass": "fa fa-check-circle-o",
            "position": 19
        }
    ],
    "components": [
        {
            "id": "requests",
            "title": "Requests",
            "enabled": true,
            "queues": {
                "Billing": "queues.queues.billing",
