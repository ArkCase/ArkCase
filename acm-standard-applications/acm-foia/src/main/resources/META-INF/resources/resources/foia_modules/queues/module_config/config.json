                        {
                            "placeholder": "from...",
                            "condition": 64,
                            "flags": {
                                "date": true
                            }
                        },
                        {
                            "placeholder": "to...",
                            "condition": 256,
                            "flags": {
                                "date": true
                            }
                        }
                    ]
                },
                {
                    "name": "queueDueDate",
                    "displayName": "queues.table.header.due",
                    "headerCellFilter": "translate",
                    "headerTooltip": "queues.table.headerTooltip.queueDueDate",
