{
  "id": "dashboard",
  "title": "Dashboard",
  "menus": [
    {
      "menuId": "leftnav",
      "menuItemURL": "dashboard",
      "menuItemTitle": "Dashboard",
      "iconClass": "fa fa-dashboard",
      "position": 0
    }
  ],
  "components": [
    {
      "id": "news",
      "title": "News Widget",
      "url": "https:\/\/query.yahooapis.com\/v1\/public\/yql",
      "query": "select * from xml where url = '$1'",
      "format": "json"
    },
    {
      "id": "weather",
      "title": "Weather Widget",
      "APPID": "672e193bbf022bdc749986f7be4c0364",
      "units": "imperial"
    },
    {
      "id": "myCases",
      "title": "My Cases Widget",
      "enableFiltering": false,
      "paginationPageSizes": [
        5,
        10,
        25
      ],
      "paginationPageSize": 5,
      "columnDefs": [
        {
          "name": "name",
          "displayName": "dashboard.widgets.myCases.table.header.caseId",
          "headerCellFilter": "translate",
          "enableSorting": true,
          "cellTemplate": "<a data-ui-sref=\"cases.main({id: row.entity.object_id_s})\">{{COL_FIELD}}<\/a>",
          "width": 130
        },
        {
          "name": "title_parseable",
		  "sortField": "title_parseable_lcs",
          "displayName": "dashboard.widgets.myCases.table.header.title",
          "headerCellFilter": "translate",
          "enableSorting": true,
          "cellTemplate": "<a data-ui-sref=\"cases.main({id: row.entity.object_id_s})\">{{COL_FIELD}}<\/a>"
        },
        {
          "name": "status_lcs",
          "displayName": "dashboard.widgets.myCases.table.header.status",
          "headerCellFilter": "translate",
          "enableSorting": true,
          "width": 150
        },
        {
          "name": "priority_lcs",
          "displayName": "dashboard.widgets.myCases.table.header.priority",
          "headerCellFilter": "translate",
          "enableSorting": true,
          "width": 70
        }
      ]
    },
    {
      "id": "myTasks",
      "title": "My Tasks Widget",
      "enableFiltering": false,
      "paginationPageSizes": [
        5,
        10,
        25
      ],
      "paginationPageSize": 5,
      "columnDefs": [
        {
          "name": "taskId",
          "displayName": "dashboard.widgets.myTasks.table.header.taskId",
          "headerCellFilter": "translate",
          "enableSorting": true,
          "type":"number",
          "cellTemplate": "<a data-ui-sref=\"tasks.main({id: row.entity.taskId})\">{{COL_FIELD}}<\/a>",
          "width": 75
        },
        {
          "name": "title",
          "displayName": "dashboard.widgets.myTasks.table.header.title",
          "headerCellFilter": "translate",
          "cellTemplate": "<a data-ui-sref=\"tasks.main({id: row.entity.taskId})\">{{COL_FIELD}}<\/a>"
        },
        {
          "name": "priority",
          "displayName": "dashboard.widgets.myTasks.table.header.priority",
          "headerCellFilter": "translate",
          "width": 75
        },
        {
          "name": "dueDate",
          "displayName": "dashboard.widgets.myTasks.table.header.dueDate",
          "headerCellFilter": "translate",
		  "cellTemplate": "<div>{{ row.entity.dueDate | date : ('common.defaultDateUIFormat' | translate)}}</div>",
          "width": 70
        },
        {
          "name": "status",
          "displayName": "dashboard.widgets.myTasks.table.header.status",
          "headerCellFilter": "translate",
          "width": 100
        },
        {
          "name": "parentObjectId",
          "displayName": "dashboard.widgets.myTasks.table.header.caseComplaintId",
          "headerCellFilter": "translate",
          "cellTemplate": "<button class='btn-link' ng-click='grid.appScope.myTasks.onClickCaseComplaintId(row.entity.parentObjectType,row.entity.parentObjectId)'>{{COL_FIELD}}</button>",
          "width": 100
        }
      ]
    },
    {
      "id": "myComplaints",
      "title": "My Complaints Widget",
      "enableFiltering": false,
      "paginationPageSizes": [
        5,
        10,
        25
      ],
      "paginationPageSize": 5,
      "columnDefs": [
        {
          "name": "complaintNumber",
          "displayName": "dashboard.widgets.myComplaints.table.header.number",
          "headerCellFilter": "translate",
          "cellTemplate": "<a data-ui-sref=\"complaints.main({id: row.entity.complaintId})\">{{COL_FIELD}}<\/a>",
          "width": 175
        },
        {
          "name": "complaintTitle",
          "displayName": "dashboard.widgets.myComplaints.table.header.title",
          "headerCellFilter": "translate",
          "cellTemplate": "<a data-ui-sref=\"complaints.main({id: row.entity.complaintId})\">{{COL_FIELD}}<\/a>"
        },
        {
          "name": "priority",
          "displayName": "dashboard.widgets.myComplaints.table.header.priority",
          "headerCellFilter": "translate",
          "width": 75
        },
        {
          "name": "created",
          "displayName": "dashboard.widgets.myComplaints.table.header.created",
          "headerCellFilter": "translate",
		  "cellTemplate": "<div>{{ row.entity.created | date : ('common.defaultDateUIFormat' | translate)}}</div>",
          "width": 70
        },
        {
          "name": "status",
          "displayName": "dashboard.widgets.myComplaints.table.header.status",
          "headerCellFilter": "translate",
          "width": 100
        }
      ]
    },
    {
      "id": "newComplaints",
      "title": "New Complaints",
      "redirectSettings": {
      }
    },
    {
      "id": "casesByQueue",
      "title": "Cases By Queue",
      "redirectSettings": {
      }
    }
  ],
  "modules": [
    {
      "name": "CASE",
      "configName": "cases"
    },
    {
      "name": "COMPLAINT",
      "configName": "complaints"
    },
    {
      "name": "COST",
      "configName": "cost-tracking"
    },
    {
      "name": "TIME",
      "configName": "time-tracking"
    },
    {
      "name": "TASK",
      "configName": "tasks"
    }
  ],
  "objectWidgets": [
    "calendar",
    "correspondence",
    "cost",
    "details",
    "docReview",
    "documents",
    "expenses",
    "history",
    "hoursSummary",
    "locations",
    "notes",
    "participants",
    "people",
    "person",
    "references",
    "reworkDetails",
    "signature",
    "tasks",
    "time",
    "workflow"
  ]
}