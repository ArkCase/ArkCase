    "filters": [
      {
        "desc": "All Open people",
        "name": "all-open-people",
        "value": "fq=-status_s:COMPLETE AND -status_s:DELETE AND -status_s:CLOSED",
        "default": true
      },
      {
        "desc": "All I've Created",
        "name": "my-people-created",
        "value": "fq=author_s:${user}"
      },
      {
        "desc": "All I'm Assigned",
        "name": "my-people-assigned",
        "value": "fq=assignee_s:${user}"
      },
      {
        "desc": "All I'm Following",
        "name": "my-people-following",
        "value": "fq={!join from=parent_object_id_s to=object_id_s}(owner_s:${user} AND object_type_s:SUBSCRIPTION)"
      },
      {
        "desc": "(No Filter)",
        "name": "",
        "value": ""
      }
