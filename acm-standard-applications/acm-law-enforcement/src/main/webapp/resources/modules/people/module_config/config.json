{
  "id": "people",
  "title": "people",
  "initialLinksShown": true,
  "hasOverviewWidgets": true,
  "menus": [
    {
      "menuId": "leftnav",
      "menuItemURL": "people.main",
      "menuItemTitle": "people.menus.leftnav.item",
      "moduleId": "person",
      "iconClass": "fa fa-exclamation-triangle",
      "position": 80
    }
  ],
  "tree": {
    "pageSize": 32,
    "filters": [
      {
        "desc": "All Open people",
        "name": "all-open-people",
        "value": "fq=-status_s:COMPLETE AND -status_s:DELETE AND -status_s:CLOSED",
        "default": true
      },
      {
        "desc": "All I've Created",
        "name": "my-people-created",
        "value": "fq=author_s:${user}"
      },
      {
        "desc": "All I'm Assigned",
        "name": "my-people-assigned",
        "value": "fq=assignee_s:${user}"
      },
      {
        "desc": "All I'm Following",
        "name": "my-people-following",
        "value": "fq={!join from=parent_object_id_s to=object_id_s}(owner_s:${user} AND object_type_s:SUBSCRIPTION)"
      },
      {
        "desc": "(No Filter)",
        "name": "",
        "value": ""
      }
    ],
    "sorters": [
      {
        "desc": "Sort Date Created Ascending",
        "name": "create_tdt ASC",
        "default": true
      },
      {
        "desc": "Sort Date Created Descending",
        "name": "create_tdt DESC"
      },
      {
        "desc": "Sort People ID Ascending",
        "name": "id ASC"
      },
      {
        "desc": "Sort People ID Descending",
        "name": "id DESC"
      },
      {
        "desc": "Sort By People Name",
        "name": "name DESC"
      }
    ],
    "nodeTypes": [
      {
        "type": "prev",
        "icon": "fa fa-arrow-up"
      },
      {
        "type": "next",
        "icon": "fa fa-arrow-down"
      },
      {
        "type": "p/PERSON",
        "icon": "fa fa-exclamation-triangle",
        "components": [
          "details",
          "pictures",
          "history"
        ]
      },
      {
        "type": "p/PERSON/det",
        "label": "Details",
        "icon": "fa fa-info-circle",
        "components": [
          "details"
        ]
      },
      {
        "type": "p/PERSON/pic",
        "label": "Pictures",
        "icon": "fa fa-info-circle",
        "components": [
          "pictures"
        ]
      },
      {
        "type": "p/PERSON/his",
        "label": "History",
        "icon": "fa fa-bell-o",
        "components": [
          "history"
        ]
      }
    ]
  },
  "components": [
    {
      "id": "main",
      "title": "Overview",
      "icon": "fa fa-tachometer",
      "enabled": false,
      "type": "empty",
      "editable": false,
      "widgets": [
      ]
    },
    {
      "id": "info",
      "title": "Info",
      "enabled": false,
      "type": "empty",
      "editable": false
    },
    {
      "id": "actions",
      "title": "Actions",
      "enabled": false,
      "type": "empty",
      "editable": false
    },
    {
      "id": "details",
      "title": "Details",
      "icon": "fa fa-info-circle",
      "enabled": true,
      "type": "empty"
    },
    {
      "id": "pictures",
      "title": "Pictures",
      "icon": "fa fa-info-circle",
      "enabled": true,
      "type": "grid",
      "enableFiltering": false,
      "paginationPageSizes": [
        5,
        10,
        20,
        50
      ],
      "paginationPageSize": 10,
      "columnDefs": [
        {
          "name": "title",
          "field": "title_parseable",
          "displayName": "people.comp.pictures.table.columns.title",
          "headerCellFilter": "translate",
          "cellTemplate": "<div ng-bind-html='row.entity.title_parseable' title='{{row.entity.title_parseable}}' class='ui-grid-cell-contents'></div>"
        },
        {
          "name": "created",
          "displayName": "people.comp.pictures.table.columns.created",
          "cellTemplate": "<div>{{ row.entity.create_date_tdt | date : ('common.defaultDateUIFormat' | translate)}}</div>",
          "headerCellFilter": "translate",
          "enableCellEdit": false,
          "width": 150
        },
        {
          "name": "author",
          "field": "creator_full_name_lcs",
          "displayName": "people.comp.pictures.table.columns.creator",
          "headerCellFilter": "translate",
          "enableCellEdit": false,
          "width": 150
        }
      ]
    },
    {
      "id": "history",
      "title": "History",
      "icon": "fa fa-bell-o",
      "enabled": true,
      "type": "grid",
      "enableFiltering": true,
      "paginationPageSizes": [
        5,
        10,
        20,
        50
      ],
      "paginationPageSize": 10,
      "columnDefs": [
        {
          "name": "eventType",
          "displayName": "people.comp.history.table.columns.eventType",
          "headerCellFilter": "translate",
          "cellTemplate": "<div> <span ng-if='row.entity.parentObjectType'>{{('common.objectTypes.'+row.entity.parentObjectType) | translate}} Updated - </span> {{row.entity.eventDescription || row.entity.eventType}}</div>"
        },
        {
          "name": "eventDate",
          "displayName": "people.comp.history.table.columns.eventDate",
          "headerCellFilter": "translate",
          "cellTemplate": "<div>{{ row.entity.eventDate | date : ('common.defaultDateTimeUIFormat' | translate)}}</div>"
        },
        {
          "name": "userId",
          "displayName": "people.comp.history.table.columns.userId",
          "headerCellFilter": "translate",
          "lookup": "userFullNames"
        }
      ]
    }
  ]
}