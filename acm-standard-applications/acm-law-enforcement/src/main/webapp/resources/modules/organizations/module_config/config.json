          "headerCellFilter": "translate"
        },
        {
          "name": "author",
          "field": "creator",
          "displayName": "organizations.comp.phones.table.columns.creator",
          "headerCellFilter": "translate",
          "lookup": "userFullNames",
          "width": 150
        },
        {
          "name": "created",
          "displayName": "organizations.comp.phones.table.columns.created",
