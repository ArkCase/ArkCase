{
  "id": "cases",
  "title": "Cases",
  "hasOverviewWidgets": true,
  "initialLinksShown": true,
  "caseInfoFilter": "\"Object Type\": CASE_FILE",
  "menus": [
    {
      "menuId": "leftnav",
      "menuItemURL": "cases.main",
      "menuItemTitle": "cases.menus.leftnav.item",
      "moduleId": "case",
      "iconClass": "fa fa-folder",
      "position": 50
    },
    {
      "menuId": "topbar",
      "menuItemURL": "frevvo.new-case",
      "menuItemTitle": "cases.menus.topbar.item",
      "moduleId": "case",
      "iconClass": "fa fa-folder",
      "position": 20
    }
  ],
  "tree": {
    "pageSize": 32,
    "filters": [
      {
        "desc": "All Open Cases",
        "name": "all-open-cases",
        "value": "fq=-status_s:COMPLETE AND -status_s:DELETE AND -status_s:CLOSED",
        "default": true
      },
      {
        "desc": "All I've Created",
        "name": "my-created-cases",
        "value": "fq=author_s:${user}"
      },
      {
        "desc": "All I'm Assigned",
        "name": "my-assigned-cases",
        "value": "fq=assignee_s:${user}"
      },
      {
        "desc": "All I'm Following",
        "name": "my-following-cases",
        "value": "fq={!join from=parent_object_id_s to=object_id_s}(owner_s:${user} AND object_type_s:SUBSCRIPTION)"
      },
      {
        "desc": "All Closed",
        "name": "all-closed-cases",
        "value": "fq=status_s:CLOSED"
      },
      {
        "desc": "(No Filter)",
        "name": "",
        "value": ""
      }
    ],
    "sorters": [
      {
        "desc": "Sort Date Asc",
        "name": "sort-date-asc",
        "value": "create_tdt ASC"
      },
      {
        "desc": "Sort Date Desc",
        "name": "sort-date-desc",
        "value": "create_tdt DESC",
        "default": true
      },
      {
        "desc": "Sort Case ID Asc",
        "name": "sort-case-id-asc",
        "value": "object_id_s ASC"
      },
      {
        "desc": "Sort Case ID Desc",
        "name": "sort-case-id-desc",
        "value": "object_id_s DESC"
      },
      {
        "desc": "Sort By Case Name",
        "name": "sort-by-name-asc",
        "value": "name ASC"
      },
      {
        "desc": "(No Sort)",
        "name": "",
        "value": ""
      }
    ],
