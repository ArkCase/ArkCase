<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

    <bean id="addReferenceAPIController"
          class="com.armedia.acm.plugins.objectassociaton.web.api.AddReferenceAPIController">
        <property name="objectAssociationService" ref="objectAssociationService"/>
        <property name="eventPublisher" ref="objectAssociationEventPublisher"/>
    </bean>
    <bean class="com.armedia.acm.plugins.objectassociaton.web.api.ObjectAssociationAPIController"
          id="objectAssociationAPIController">
        <property name="objectAssociationService" ref="objectAssociationService"/>
    </bean>

</beans>
