<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <bean id="niemExportUtils" class="com.armedia.acm.plugins.report.niem.NiemExportUtils"/>

    <bean id="csvToMapReader" class="com.armedia.acm.plugins.report.niem.CSVToMapReader"/>

    <bean id="niemExportService" class="com.armedia.acm.plugins.report.niem.NiemExportService">
        <property name="niemExportUtils" ref="niemExportUtils"/>
        <property name="componentMap">
            <map>
                <entry key="FOIA" value="ORG1"/>
                <entry key="EEOC" value="ORG2"/>
            </map>
        </property>
    </bean>

</beans>