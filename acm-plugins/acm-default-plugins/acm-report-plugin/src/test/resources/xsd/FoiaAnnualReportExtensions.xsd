<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:foia="http://leisp.usdoj.gov/niem/FoiaAnnualReport/extension/1.03" xmlns:i="http://niem.gov/niem/appinfo/2.0" xmlns:j="http://niem.gov/niem/domains/jxdm/4.1" xmlns:nc="http://niem.gov/niem/niem-core/2.0" xmlns:niem-xsd="http://niem.gov/niem/proxy/xsd/2.0" xmlns:s="http://niem.gov/niem/structures/2.0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://leisp.usdoj.gov/niem/FoiaAnnualReport/extension/1.03" elementFormDefault="qualified" attributeFormDefault="qualified" version="1.0.0">
   <xsd:import namespace="http://niem.gov/niem/appinfo/2.0" schemaLocation="../Subset/niem/appinfo/2.0/appinfo.xsd"/>
   <xsd:import namespace="http://niem.gov/niem/domains/jxdm/4.1" schemaLocation="../Subset/niem/domains/jxdm/4.1/jxdm.xsd"/>
   <xsd:import namespace="http://niem.gov/niem/niem-core/2.0" schemaLocation="../Subset/niem/niem-core/2.0/niem-core.xsd"/>
   <xsd:import namespace="http://niem.gov/niem/proxy/xsd/2.0" schemaLocation="../Subset/niem/proxy/xsd/2.0/xsd.xsd"/>
   <xsd:import namespace="http://niem.gov/niem/structures/2.0" schemaLocation="../Subset/niem/structures/2.0/structures.xsd"/>
   <xsd:annotation>
      <xsd:documentation>Extension XML Schema for the FOIA Annual Report NIEM IEPD</xsd:documentation>
      <xsd:appinfo>
         <i:ConformantIndicator>true</i:ConformantIndicator>
      </xsd:appinfo>
   </xsd:annotation>
   <xsd:complexType name="AppealDispositionAppliedExemptionsSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section showing the number of times exemptions were applied in denying appeals. Corresponds to section VI.C.1 of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:ComponentAppliedExemptions" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:ComponentAppliedExemptionsOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="AppealDispositionSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section describing disposition of administrative appeals. Corresponds to section VI.B of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:AppealDisposition" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:AppealDispositionOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="AppealDispositionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a description of disposition of administrative appeals by an agency or component.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:AppealDispositionAffirmedQuantity"/>
               <xsd:element ref="foia:AppealDispositionPartialQuantity"/>
               <xsd:element ref="foia:AppealDispositionReversedQuantity"/>
               <xsd:element ref="foia:AppealDispositionOtherQuantity"/>
               <xsd:element ref="foia:AppealDispositionTotalQuantity"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="AppealNonExemptionDenialSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section describing non-exemption reasons for denial on appeal. Corresponds to section VI.C.2 of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:AppealNonExemptionDenial" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:AppealNonExemptionDenialOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="AppealNonExemptionDenialType">
      <xsd:annotation>
         <xsd:documentation>A data type for a set of non-exemption appeal denials associated with a component.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:NonExemptionDenial" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="AppealResponseTimeSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section providing statistics for administrative appeal response time. Corresponds to section VI.C.4 of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:ResponseTime" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:ResponseTimeOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="AppliedExemptionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a FOIA exemption and the number of times the exemption was relied upon.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="foia:AppliedExemptionCode"/>
               <xsd:element ref="foia:AppliedExemptionQuantity"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="BacklogComparisonSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section comparing current year backlog to previous year. Corresponds to sections XII.D.2 and XII.E.2 of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:BacklogComparison" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:BacklogComparisonOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="BacklogComparisonType">
      <xsd:annotation>
         <xsd:documentation>A data type for a comparison between current and previous year backlog for an agency or component.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:BacklogLastYearQuantity"/>
               <xsd:element ref="foia:BacklogCurrentYearQuantity"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="BacklogSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section showing the number of items that were pending beyond the statutory time period. Corresponds to section XII.A of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:Backlog" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:BacklogOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="BacklogType">
      <xsd:annotation>
         <xsd:documentation>A data type for a detailing of the number of items pending beyond the statutory time period for an agency or component.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:BackloggedRequestQuantity" minOccurs="0"/>
               <xsd:element ref="foia:BackloggedAppealQuantity" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ComponentAppliedExemptionsType">
      <xsd:annotation>
         <xsd:documentation>A data type for an enumeration of a component's reliance upon exemptions for denial of requests.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:AppliedExemption" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ComponentDataOrganizationAssociationType">
      <xsd:annotation>
         <xsd:documentation>A data type for a relationship between a set of data and a "component" (agency, sub-organization, office, or division).</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/niem-core/2.0" i:name="AssociationType"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="nc:AssociationType">
            <xsd:sequence>
               <xsd:element ref="foia:ComponentDataReference"/>
               <xsd:element ref="nc:OrganizationReference"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ComponentDataType" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a set of data associated with a "component" (agency, sub-organization, office, or division).</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType"/>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ComponentOtherDenialReasonType">
      <xsd:annotation>
         <xsd:documentation>A data type for a description of denial reasons other than pre-defined reasons.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="foia:OtherDenialReason" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:ComponentOtherDenialReasonQuantity"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ComponentResponseTimeIncrementsType">
      <xsd:annotation>
         <xsd:documentation>A data type for an agency or component's breakdown of response times into 13 designated categories.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:TimeIncrement" minOccurs="13" maxOccurs="13"/>
               <xsd:element ref="foia:TimeIncrementTotalQuantity"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="Exemption3StatuteSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section listing exemption 3 statutes an agency relied upon to withhold information. Corresponds to section IV of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:ReliedUponStatute" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:ReliedUponStatuteOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="ExemptionCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for a FOIA exemption.</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="Ex. 1">
            <xsd:annotation>
               <xsd:documentation>Exemption 1</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Ex. 2">
            <xsd:annotation>
               <xsd:documentation>Exemption 2</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Ex. 3">
            <xsd:annotation>
               <xsd:documentation>Exemption 3</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Ex. 4">
            <xsd:annotation>
               <xsd:documentation>Exemption 4</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Ex. 5">
            <xsd:annotation>
               <xsd:documentation>Exemption 5</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Ex. 6">
            <xsd:annotation>
               <xsd:documentation>Exemption 6</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Ex. 7(A)">
            <xsd:annotation>
               <xsd:documentation>Exemption 7(A)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Ex. 7(B)">
            <xsd:annotation>
               <xsd:documentation>Exemption 7(B)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Ex. 7(C)">
            <xsd:annotation>
               <xsd:documentation>Exemption 7(C)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Ex. 7(D)">
            <xsd:annotation>
               <xsd:documentation>Exemption 7(D)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Ex. 7(E)">
            <xsd:annotation>
               <xsd:documentation>Exemption 7(E)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Ex. 7(F)">
            <xsd:annotation>
               <xsd:documentation>Exemption 7(F)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Ex. 8">
            <xsd:annotation>
               <xsd:documentation>Exemption 8</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Ex. 9">
            <xsd:annotation>
               <xsd:documentation>Exemption 9</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="ExemptionCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a FOIA exemption.</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="foia:ExemptionCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:complexType name="ExpeditedProcessingSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section describing handling of adjudicated requests for expedited processing. Corresponds to section VIII.A of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:ExpeditedProcessing" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:ExpeditedProcessingOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ExpeditedProcessingType">
      <xsd:annotation>
         <xsd:documentation>A data type for a description of handling of adjudicated requests for expedited processing by an agency or component.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:RequestGrantedQuantity"/>
               <xsd:element ref="foia:RequestDeniedQuantity"/>
               <xsd:element ref="foia:AdjudicationMedianDaysValue" minOccurs="0"/>
               <xsd:element ref="foia:AdjudicationAverageDaysValue" minOccurs="0"/>
               <xsd:element ref="foia:AdjudicationWithinTenDaysQuantity" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="FeesCollectedSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section showing fees collected from FOIA requesters for processing their requests. Corresponds to section X of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:FeesCollected" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FeesCollectedOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="FeesCollectedType">
      <xsd:annotation>
         <xsd:documentation>A data type for a detailing of fees collected by an agency or component from FOIA requesters for processing their requests.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:FeesCollectedAmount"/>
               <xsd:element ref="foia:FeesCollectedCostPercent"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="FeeWaiverSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section describing handling of requests for a waiver of fees. Corresponds to section VIII.B of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:FeeWaiver" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FeeWaiverOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="FeeWaiverType">
      <xsd:annotation>
         <xsd:documentation>A data type for a description of handling requests for a waiver of fees by an agency or component.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:RequestGrantedQuantity"/>
               <xsd:element ref="foia:RequestDeniedQuantity"/>
               <xsd:element ref="foia:AdjudicationMedianDaysValue" minOccurs="0"/>
               <xsd:element ref="foia:AdjudicationAverageDaysValue" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="NonExemptionDenialReasonCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for a reason for denial of FOIA requests other than exemptions.</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="NoRecords">
            <xsd:annotation>
               <xsd:documentation>No Records</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Referred">
            <xsd:annotation>
               <xsd:documentation>All Records Referred to Another Component or Agency</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Withdrawn">
            <xsd:annotation>
               <xsd:documentation>Request Withdrawn</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="FeeRelated">
            <xsd:annotation>
               <xsd:documentation>Fee-Related Reason</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="NotDescribed">
            <xsd:annotation>
               <xsd:documentation>Records not Reasonably Described</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="ImproperRequest">
            <xsd:annotation>
               <xsd:documentation>Improper FOIA Request for Other Reason</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="NotAgency">
            <xsd:annotation>
               <xsd:documentation>Not Agency Record</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Duplicate">
            <xsd:annotation>
               <xsd:documentation>Duplicate Request</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="InLitigation">
            <xsd:annotation>
               <xsd:documentation>Request in Litigation</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="ExpeditedDenial">
            <xsd:annotation>
               <xsd:documentation>Appeal base solely on denial of expedited processing</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Other">
            <xsd:annotation>
               <xsd:documentation>Other Reason (explained in section V.B.2)</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="NonExemptionDenialReasonCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a reason for denial of FOIA requests other than exemptions.</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="foia:NonExemptionDenialReasonCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:complexType name="NonExemptionDenialType">
      <xsd:annotation>
         <xsd:documentation>A data type for a non-exemption reason and count of denials of FOIA requests.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="foia:NonExemptionDenialReasonCode"/>
               <xsd:element ref="foia:NonExemptionDenialQuantity"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="OldestPendingItemSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section showing the oldest pending items of some type (request, appeal, consultation). Corresponds to sections VI.C.5, VII.E, and XII.C of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:OldestPendingItems" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:OldestPendingItemsOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="OldestPendingItemsType">
      <xsd:annotation>
         <xsd:documentation>A data type for an enumeration of the oldest pending items of some type (request, appeal, consultation) for an agency or component.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:OldItem" minOccurs="0" maxOccurs="10"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="OldItemType">
      <xsd:annotation>
         <xsd:documentation>A data type for a receipt date and age of an old item (request, appeal, consultation).</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="foia:OldItemReceiptDate"/>
               <xsd:element ref="foia:OldItemPendingDaysQuantity"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="OtherDenialReasonSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section describing 'other' reasons for denial. Corresponds to sections V.B.2 and VI.C.3 of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:ComponentOtherDenialReason" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:OtherDenialReasonOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="OtherDenialReasonType">
      <xsd:annotation>
         <xsd:documentation>A data type for a reason requests or appeals were denied, and the number of times the reason was relied upon.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="foia:OtherDenialReasonDescriptionText"/>
               <xsd:element ref="foia:OtherDenialReasonQuantity"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="PendingPerfectedRequestsSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section providing statistics for pending perfected requests at end of fiscal year. Corresponds to section VII.D of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:PendingPerfectedRequests" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:PendingPerfectedRequestsOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="PendingPerfectedRequestsType">
      <xsd:annotation>
         <xsd:documentation>A data type for a set of statistics regarding pending perfected requests at end of fiscal year, separated out by track (simple, complex, expedited).</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:SimplePendingRequestStatistics"/>
               <xsd:element ref="foia:ComplexPendingRequestStatistics"/>
               <xsd:element ref="foia:ExpeditedPendingRequestStatistics"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="PendingRequestStatisticsType">
      <xsd:annotation>
         <xsd:documentation>A data type for a set of statistics regarding pending perfected requests at end of fiscal year.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="foia:PendingRequestQuantity"/>
               <xsd:element ref="foia:PendingRequestMedianDaysValue" minOccurs="0"/>
               <xsd:element ref="foia:PendingRequestAverageDaysValue" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="PersonnelAndCostSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section showing personnel dedicated to, and costs associated with FOIA duties. Corresponds to section IX of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:PersonnelAndCost" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:PersonnelAndCostOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="PersonnelAndCostType">
      <xsd:annotation>
         <xsd:documentation>A data type for a description of personnel dedicated to, and costs associated with FOIA duties for an agency or component.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:FullTimeEmployeeQuantity"/>
               <xsd:element ref="foia:EquivalentFullTimeEmployeeQuantity"/>
               <xsd:element ref="foia:TotalFullTimeStaffQuantity"/>
               <xsd:element ref="foia:ProcessingCostAmount"/>
               <xsd:element ref="foia:LitigationCostAmount"/>
               <xsd:element ref="foia:TotalCostAmount"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ProcessedResponseTimeSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section providing statistics for response times to FOIA requests. Corresponds to sections VII.A and VII.B of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:ProcessedResponseTime" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:ProcessedResponseTimeOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ProcessedResponseTimeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a set of response time statistics for requests over the course of the fiscal year, separated out by track (simple, complex, expedited).</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:SimpleResponseTime"/>
               <xsd:element ref="foia:ComplexResponseTime"/>
               <xsd:element ref="foia:ExpeditedResponseTime"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ProcessedSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section describing processing statistics for requests, appeals, or consultations over the course of the fiscal year. Corresponds to sections V.A, VI.A, and VII.B of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:ProcessingStatistics" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:ProcessingStatisticsOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ProcessingComparisonSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section comparing current year processing of items (requests, appeals) to previous year. Corresponds to sections XII.D.1 and XII.E.1 of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:ProcessingComparison" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:ProcessingComparisonOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ProcessingComparisonType">
      <xsd:annotation>
         <xsd:documentation>A data type for a comparison between current and previous year processing of items (requests, appeals) for an agency or component.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:ItemsReceivedLastYearQuantity"/>
               <xsd:element ref="foia:ItemsReceivedCurrentYearQuantity"/>
               <xsd:element ref="foia:ItemsProcessedLastYearQuantity"/>
               <xsd:element ref="foia:ItemsProcessedCurrentYearQuantity"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ProcessingStatisticsType">
      <xsd:annotation>
         <xsd:documentation>A data type for a set of processing statistics regarding different types of items (requests, appeals, consultations) over the course of the fiscal year.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:ProcessingStatisticsPendingAtStartQuantity"/>
               <xsd:element ref="foia:ProcessingStatisticsReceivedQuantity"/>
               <xsd:element ref="foia:ProcessingStatisticsProcessedQuantity"/>
               <xsd:element ref="foia:ProcessingStatisticsPendingAtEndQuantity"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ReliedUponStatuteOrganizationAssociationType">
      <xsd:annotation>
         <xsd:documentation>A data type for a relationship between a statute and a component or agency. Includes a count of the number of times the agency or component relied upon the statute.</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://leisp.usdoj.gov/niem/FoiaAnnualReport/extension/1.03" i:name="ComponentDataOrganizationAssociationType"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataOrganizationAssociationType">
            <xsd:sequence>
               <xsd:element ref="foia:ReliedUponStatuteQuantity"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ReliedUponStatuteType">
      <xsd:annotation>
         <xsd:documentation>A data type for an exemption 3 statute an agency relied upon to withhold information.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="j:StatuteDescriptionText"/>
               <xsd:element ref="foia:ReliedUponStatuteInformationWithheldText" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="nc:Case" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ReportSectionType" abstract="true">
      <xsd:annotation>
         <xsd:documentation>A data concept for a single section of a FOIA Annual Report</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType"/>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="RequestDispositionAppliedExemptionsSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section showing the number of times exemptions were applied in denying requests. Corresponds to section V.B.3 of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:ComponentAppliedExemptions" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:ComponentAppliedExemptionsOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="RequestDispositionSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section describing disposition of processed FOIA requests. Corresponds to section V.B.1 of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:RequestDisposition" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:RequestDispositionOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="RequestDispositionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a set of statistics describing disposition of processed FOIA requests by an agency or component.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:RequestDispositionFullGrantQuantity"/>
               <xsd:element ref="foia:RequestDispositionPartialGrantQuantity"/>
               <xsd:element ref="foia:RequestDispositionFullExemptionDenialQuantity"/>
               <xsd:element ref="foia:NonExemptionDenial" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:RequestDispositionTotalQuantity"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="ResponseTimeCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for a special non-numeric response time value.</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="LT1">
            <xsd:annotation>
               <xsd:documentation>Between 0 and 1</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="ResponseTimeCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a special non-numeric response time value.</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="foia:ResponseTimeCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:complexType name="ResponseTimeIncrementsSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section showing breakdown of response times into 13 designated categories. Corresponds to sections VII.C.1, VII.C.2, and VII.C.3 of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:ComponentResponseTimeIncrements" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:ResponseTimeIncrementsOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="ResponseTimeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a set of response time statistics regarding different types of items (requests, appeals) over the course of the fiscal year.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:ResponseTimeMedianDays" minOccurs="0"/>
               <xsd:element ref="foia:ResponseTimeAverageDays" minOccurs="0"/>
               <xsd:element ref="foia:ResponseTimeLowestDays" minOccurs="0"/>
               <xsd:element ref="foia:ResponseTimeHighestDays" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>   
   <xsd:complexType name="SubsectionPostSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section showing the number of items that were posted by FOIA Office/Program Office. Corresponds to section XI.B of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:Subsection" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:SubsectionPostOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SubsectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a detailing of the number of items that were posted by FOIA Office/Program Office for an agency or component.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:PostedbyFOIAQuantity" minOccurs="0"/>
               <xsd:element ref="foia:PostedbyProgramQuantity" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SubsectionUsedSectionType">
      <xsd:annotation>
         <xsd:documentation>A data type for a report section showing the number of times that were used. Corresponds to section XI.A of the guidance document.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ReportSectionType">
            <xsd:sequence>
               <xsd:element ref="foia:SubsectionUsed" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:SubsectionUsedOrganizationAssociation" minOccurs="0" maxOccurs="unbounded"/>
               <xsd:element ref="foia:FootnoteText" minOccurs="0" maxOccurs="unbounded"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="SubsectionUsedType">
      <xsd:annotation>
         <xsd:documentation>A data type for a detailing of the number of times that were used for an agency or component.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="foia:ComponentDataType">
            <xsd:sequence>
               <xsd:element ref="foia:TimesUsedQuantity" minOccurs="0"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="TimeIncrementCodeSimpleType">
      <xsd:annotation>
         <xsd:documentation>A data type for a specific response time increment.</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:token">
         <xsd:enumeration value="1-20">
            <xsd:annotation>
               <xsd:documentation>1-20 Days</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="21-40">
            <xsd:annotation>
               <xsd:documentation>21-40 Days</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="41-60">
            <xsd:annotation>
               <xsd:documentation>41-60 Days</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="61-80">
            <xsd:annotation>
               <xsd:documentation>61-80 Days</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="81-100">
            <xsd:annotation>
               <xsd:documentation>81-100 Days</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="101-120">
            <xsd:annotation>
               <xsd:documentation>101-120 Days</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="121-140">
            <xsd:annotation>
               <xsd:documentation>121-140 Days</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="141-160">
            <xsd:annotation>
               <xsd:documentation>141-160 Days</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="161-180">
            <xsd:annotation>
               <xsd:documentation>161-180 Days</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="181-200">
            <xsd:annotation>
               <xsd:documentation>181-200 Days</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="201-300">
            <xsd:annotation>
               <xsd:documentation>201-300 Days</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="301-400">
            <xsd:annotation>
               <xsd:documentation>301-400 Days</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="401+">
            <xsd:annotation>
               <xsd:documentation>401+ Days</xsd:documentation>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:complexType name="TimeIncrementCodeType">
      <xsd:annotation>
         <xsd:documentation>A data type for a specific response time increment.</xsd:documentation>
         <xsd:appinfo>
            <i:Base i:namespace="http://niem.gov/niem/structures/2.0" i:name="Object"/>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:extension base="foia:TimeIncrementCodeSimpleType">
            <xsd:attributeGroup ref="s:SimpleObjectAttributeGroup"/>
         </xsd:extension>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:complexType name="TimeIncrementType">
      <xsd:annotation>
         <xsd:documentation>A data type for a count of requests which were processed within a specified time increment.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="s:ComplexObjectType">
            <xsd:sequence>
               <xsd:element ref="foia:TimeIncrementCode"/>
               <xsd:element ref="foia:TimeIncrementProcessedQuantity"/>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:element name="AdjudicationAverageDaysValue" type="niem-xsd:decimal">
      <xsd:annotation>
         <xsd:documentation>A count of the average number of days taken to adjudicate requests.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AdjudicationMedianDaysValue" type="niem-xsd:decimal">
      <xsd:annotation>
         <xsd:documentation>A count of the median number of days taken to adjudicate requests.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AdjudicationWithinTenDaysQuantity" type="niem-xsd:nonNegativeInteger">
      <xsd:annotation>
         <xsd:documentation>A count of the number of requests which were adjudicated within ten calendar days.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AppealDenialOtherReasonSection" type="foia:OtherDenialReasonSectionType">
      <xsd:annotation>
         <xsd:documentation>A report section describing 'other' reasons for appeal denials based on reasons other than exemptions.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AppealDisposition" type="foia:AppealDispositionType">
      <xsd:annotation>
         <xsd:documentation>A set of values describing disposition of administrative appeals by an agency or component.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AppealDispositionAffirmedQuantity" type="niem-xsd:nonNegativeInteger">
      <xsd:annotation>
         <xsd:documentation>A count of the number of appeals which were fully affirmed.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AppealDispositionAppliedExemptionsSection" type="foia:AppealDispositionAppliedExemptionsSectionType">
      <xsd:annotation>
         <xsd:documentation>A report section showing the number of times exemptions were applied in denying appeals.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AppealDispositionOrganizationAssociation" type="foia:ComponentDataOrganizationAssociationType">
      <xsd:annotation>
         <xsd:documentation>A relationship between a set of values describing disposition of administrative appeals and an agency or component.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AppealDispositionOtherQuantity" type="niem-xsd:nonNegativeInteger">
      <xsd:annotation>
         <xsd:documentation>A count of the number of appeals which were closed for other reasons.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AppealDispositionPartialQuantity" type="niem-xsd:nonNegativeInteger">
      <xsd:annotation>
         <xsd:documentation>A count of the number of appeals which partially affirmed and partially reversed/remanded.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AppealDispositionReversedQuantity" type="niem-xsd:nonNegativeInteger">
      <xsd:annotation>
         <xsd:documentation>A count of the number of appeals which were completely reversed/remanded.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AppealDispositionSection" type="foia:AppealDispositionSectionType">
      <xsd:annotation>
         <xsd:documentation>A report section describing disposition of administrative appeals.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AppealDispositionTotalQuantity" type="niem-xsd:nonNegativeInteger">
      <xsd:annotation>
         <xsd:documentation>A count of the total number of administrative appeals.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AppealNonExemptionDenial" type="foia:AppealNonExemptionDenialType">
      <xsd:annotation>
         <xsd:documentation>A set of non-exemption appeal denials associated with a component.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AppealNonExemptionDenialOrganizationAssociation" type="foia:ComponentDataOrganizationAssociationType">
      <xsd:annotation>
         <xsd:documentation>A relationship between a set non-exemption appeal denials and an agency or component.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AppealNonExemptionDenialSection" type="foia:AppealNonExemptionDenialSectionType">
      <xsd:annotation>
         <xsd:documentation>A report section describing non-exemption reasons for denial on appeal.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
   <xsd:element name="AppealResponseTimeSection" type="foia:AppealResponseTimeSectionType">
      <xsd:annotation>
         <xsd:documentation>A report section providing statistics for administrative appeal response time.</xsd:documentation>
