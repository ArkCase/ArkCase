<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

        <bean id="calendarService" class="com.armedia.acm.calendar.service.integration.exchange.ExchangeCalendarService">
            <property name="entityHandlers">
                <map>
                    <entry key="CASE_FILE" value-ref="caseFileDao"/>
                </map>
            </property>
            <property name="outlookService" value="outlookService"/>
            <property name="outlookDao" ref="exchangeWebServicesOutlookDao"/>
        </bean>

</beans>