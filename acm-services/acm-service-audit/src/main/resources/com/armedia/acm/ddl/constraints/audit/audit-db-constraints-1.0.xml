<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
            http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="01-create-audit-table" author="dmiller">

        <addForeignKeyConstraint
                baseTableName="acm_audit_log_property"
                baseColumnNames="cm_audit_id"
                constraintName="fk_audit_property_audit_log"
                referencedTableName="acm_audit_log"
                referencedColumnNames="cm_audit_id"
                deferrable="true"
                initiallyDeferred="true" />
    </changeSet>


</databaseChangeLog>

