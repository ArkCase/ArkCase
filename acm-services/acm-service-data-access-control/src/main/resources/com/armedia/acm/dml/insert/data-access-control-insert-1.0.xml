<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="
            http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <property  name="timestampFunction" value="SYSTIMESTAMP" dbms="oracle"/>
    <property  name="timestampFunction" value="localtimestamp" dbms="postgresql"/>

    <property name="nextValFromSequence" value="SEQ_GEN_IDENTITY.NEXTVAL" dbms="oracle"/>
    <property name="nextValFromSequence" value="nextVal('SEQ_GEN_IDENTITY')" dbms="postgresql"/>

    <property name="timestampType" value="TIMESTAMP WITH TIME ZONE" dbms="oracle,postgresql"/>
    <property name="timestampType" value="TIMESTAMP" dbms="mysql"/>

    <property name="idType" value="NUMBER(32,0)" dbms="oracle"/>
    <property name="idType" value="BIGINT" dbms="mysql"/>
    <property name="idType" value="BIGSERIAL" dbms="postgresql"/>

    <changeSet id="01-data-access-acl-insert" author="dmiller" dbms="oracle,postgresql">
        <insert tableName="acm_access_control_decision_lu">
            <column name="cm_id" valueComputed="${nextValFromSequence}"/>
            <column name="cm_value" value="GRANT"/>
            <column name="cm_order" valueNumeric="0"/>
            <column name="cm_status" value="ACTIVE"/>
            <column name="cm_created" valueComputed="${timestampFunction}"/>
            <column name="cm_creator" valueComputed="USER"/>
            <column name="cm_modified" valueComputed="${timestampFunction}"/>
            <column name="cm_modifier" valueComputed="USER"/>
        </insert>
        <insert tableName="acm_access_control_decision_lu">
            <column name="cm_id" valueComputed="${nextValFromSequence}"/>
            <column name="cm_value" value="DENY"/>
            <column name="cm_order" valueNumeric="1"/>
            <column name="cm_status" value="ACTIVE"/>
            <column name="cm_created" valueComputed="${timestampFunction}"/>
            <column name="cm_creator" valueComputed="USER"/>
            <column name="cm_modified" valueComputed="${timestampFunction}"/>
            <column name="cm_modifier" valueComputed="USER"/>
        </insert>
        <insert tableName="acm_access_control_decision_lu">
            <column name="cm_id" valueComputed="${nextValFromSequence}"/>
            <column name="cm_value" value="MANDATORY_GRANT"/>
            <column name="cm_order" valueNumeric="2"/>
            <column name="cm_status" value="ACTIVE"/>
            <column name="cm_created" valueComputed="${timestampFunction}"/>
            <column name="cm_creator" valueComputed="USER"/>
            <column name="cm_modified" valueComputed="${timestampFunction}"/>
            <column name="cm_modifier" valueComputed="USER"/>
        </insert>
        <insert tableName="acm_access_control_decision_lu">
            <column name="cm_id" valueComputed="${nextValFromSequence}"/>
            <column name="cm_value" value="MANDATORY_DENY"/>
            <column name="cm_order" valueNumeric="3"/>
            <column name="cm_status" value="ACTIVE"/>
            <column name="cm_created" valueComputed="${timestampFunction}"/>
            <column name="cm_creator" valueComputed="USER"/>
            <column name="cm_modified" valueComputed="${timestampFunction}"/>
            <column name="cm_modifier" valueComputed="USER"/>
        </insert>

        <insert tableName="acm_access_control_reason_lu">
            <column name="cm_id" valueComputed="${nextValFromSequence}"/>
            <column name="cm_value" value="Default"/>
            <column name="cm_order" valueNumeric="0"/>
            <column name="cm_status" value="ACTIVE"/>
            <column name="cm_created" valueComputed="${timestampFunction}"/>
            <column name="cm_creator" valueComputed="USER"/>
            <column name="cm_modified" valueComputed="${timestampFunction}"/>
            <column name="cm_modifier" valueComputed="USER"/>
        </insert>
        <insert tableName="acm_access_control_reason_lu">
            <column name="cm_id" valueComputed="${nextValFromSequence}"/>
            <column name="cm_value" value="Discretionary"/>
            <column name="cm_order" valueNumeric="1"/>
            <column name="cm_status" value="ACTIVE"/>
            <column name="cm_created" valueComputed="${timestampFunction}"/>
            <column name="cm_creator" valueComputed="USER"/>
            <column name="cm_modified" valueComputed="${timestampFunction}"/>
            <column name="cm_modifier" valueComputed="USER"/>
        </insert>
        <insert tableName="acm_access_control_reason_lu">
            <column name="cm_id" valueComputed="${nextValFromSequence}"/>
            <column name="cm_value" value="Inherited"/>
            <column name="cm_order" valueNumeric="2"/>
            <column name="cm_status" value="ACTIVE"/>
            <column name="cm_created" valueComputed="${timestampFunction}"/>
            <column name="cm_creator" valueComputed="USER"/>
            <column name="cm_modified" valueComputed="${timestampFunction}"/>
            <column name="cm_modifier" valueComputed="USER"/>
        </insert>
        <insert tableName="acm_access_control_reason_lu">
            <column name="cm_id" valueComputed="${nextValFromSequence}"/>
            <column name="cm_value" value="Business Rule"/>
            <column name="cm_order" valueNumeric="3"/>
            <column name="cm_status" value="ACTIVE"/>
            <column name="cm_created" valueComputed="${timestampFunction}"/>
            <column name="cm_creator" valueComputed="USER"/>
            <column name="cm_modified" valueComputed="${timestampFunction}"/>
            <column name="cm_modifier" valueComputed="USER"/>
        </insert>

        <insert tableName="acm_boolean_lu">
            <column name="cm_id" valueComputed="${nextValFromSequence}"/>
            <column name="cm_value" value="Y"/>
            <column name="cm_order" valueNumeric="0"/>
            <column name="cm_status" value="ACTIVE"/>
            <column name="cm_created" valueComputed="${timestampFunction}"/>
            <column name="cm_creator" valueComputed="USER"/>
            <column name="cm_modified" valueComputed="${timestampFunction}"/>
            <column name="cm_modifier" valueComputed="USER"/>
        </insert>
        <insert tableName="acm_boolean_lu">
            <column name="cm_id" valueComputed="${nextValFromSequence}"/>
            <column name="cm_value" value="N"/>
            <column name="cm_order" valueNumeric="1"/>
            <column name="cm_status" value="ACTIVE"/>
            <column name="cm_created" valueComputed="${timestampFunction}"/>
            <column name="cm_creator" valueComputed="USER"/>
            <column name="cm_modified" valueComputed="${timestampFunction}"/>
            <column name="cm_modifier" valueComputed="USER"/>
        </insert>

    </changeSet>



    <changeSet id="02-data-access-change-boolean-values" author="dmiller" dbms="oracle,postgresql">
        <delete tableName="acm_boolean_lu"/>
        <insert tableName="acm_boolean_lu">
            <column name="cm_id" valueComputed="${nextValFromSequence}"/>
            <column name="cm_value" value="true"/>
            <column name="cm_order" valueNumeric="0"/>
            <column name="cm_status" value="ACTIVE"/>
            <column name="cm_created" valueComputed="${timestampFunction}"/>
            <column name="cm_creator" valueComputed="USER"/>
            <column name="cm_modified" valueComputed="${timestampFunction}"/>
            <column name="cm_modifier" valueComputed="USER"/>
        </insert>
        <insert tableName="acm_boolean_lu">
            <column name="cm_id" valueComputed="${nextValFromSequence}"/>
            <column name="cm_value" value="false"/>
            <column name="cm_order" valueNumeric="1"/>
            <column name="cm_status" value="ACTIVE"/>
            <column name="cm_created" valueComputed="${timestampFunction}"/>
            <column name="cm_creator" valueComputed="USER"/>
            <column name="cm_modified" valueComputed="${timestampFunction}"/>
            <column name="cm_modifier" valueComputed="USER"/>
        </insert>
    </changeSet>
</databaseChangeLog>