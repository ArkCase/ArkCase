<?xml version="1.0" encoding="US-ASCII"?>
<xml-bindings xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xmlns:xs="http://www.w3.org/2001/XMLSchema"
              package-name="com.armedia.acm.dynamic">

    <java-types>

        <java-type name="Batch">
            <xml-root-element name="Batch"/>
            <java-attributes>
                <xml-element java-attribute="BatchInstanceIdentifier" type="java.lang.String"/>
                <xml-element java-attribute="BatchClassIdentifier" type="java.lang.String"/>
                <xml-element java-attribute="BatchClassName" type="java.lang.String"/>
				<xml-element java-attribute="Documents" name="Document" type="com.armedia.acm.dynamic.Document" container-type="java.util.List">
                    <xml-element-wrapper/>
                </xml-element>
                <xml-element java-attribute="BatchLevelFields" name="BatchLevelField" type="com.armedia.acm.dynamic.BatchLevelField" container-type="java.util.List">
                    <xml-element-wrapper/>
                </xml-element>
            </java-attributes>
        </java-type>
		
		<java-type name="Document">
            <java-attributes>
				<xml-element java-attribute="Identifier" type="java.lang.String"/>
				<xml-element java-attribute="Type" type="java.lang.String"/>
				<xml-element java-attribute="Description" type="java.lang.String"/>
				<xml-element java-attribute="DocumentLevelFields" name="DocumentLevelField" type="com.armedia.acm.dynamic.DocumentLevelField" container-type="java.util.List">
                    <xml-element-wrapper/>
                </xml-element>
                <xml-element java-attribute="MultiPagePdfFile" type="java.lang.String"/>
            </java-attributes>
        </java-type>
		
		<java-type name="DocumentLevelField">
            <java-attributes>
				<xml-element java-attribute="Name" type="java.lang.String"/>
				<xml-element java-attribute="Value" type="java.lang.String"/>
            </java-attributes>
        </java-type>

        <java-type name="BatchLevelField">
            <java-attributes>
                <xml-element java-attribute="Name" type="java.lang.String"/>
                <xml-element java-attribute="Value" type="java.lang.String"/>
                <xml-element java-attribute="Type" type="java.lang.String"/>
            </java-attributes>
        </java-type>

    </java-types>

</xml-bindings>