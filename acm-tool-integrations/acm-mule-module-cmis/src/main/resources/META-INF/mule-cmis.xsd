        <xs:attribute name="endpoint" default="ATOM" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>The type of endpoint. Values: SOAP or ATOMPUB</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="connectionTimeout" default="10000" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>The connection time-out specification</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name="useAlfrescoExtension" default="false" use="optional" type="xs:string">
          <xs:annotation>
            <xs:documentation>Specifies whether the Alfresco Object Factory implementation should be utilized, the Alfresco CMIS extension JAR must be included in your Mule application in order for this configuration to work.</xs:documentation>
          </xs:annotation>
