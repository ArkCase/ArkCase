{
  "id": "tasks",
  "title": "Tasks",
  "menus": [
    {
      "menuId": "leftnav",
      "menuItemURL": "tasks",
      "menuItemTitle": "Tasks",
      "iconClass": "fa fa-check-circle",
      "position": 60
    },
    {
      "menuId": "topbar",
      "menuItemURL": "tasks.wizard",
      "menuItemTitle": "Task",
      "iconClass": "fa fa-check-circle",
      "position": 30
    }
  ],
  "tree": {
    "pageSize": 32,
    "filters": [
      {
        "desc": "Tasks for Me",
        "name": "my-tasks",
        "value": "fq=assignee_s:${user}"
      },
      {
        "desc": "All Open Tasks",
        "name": "opened-tasks",
        "value": "fq=status_s:OPEN"
      },
      {
        "desc": "All Completed Tasks",
        "name": "completed-tasks",
        "value": "fq=status_s:COMPLETE"
      },
      {
        "desc": "All Tasks Due Today",
        "name": "tasks-due-today",
        "value": "fq=due_tdt:[NOW/DAY TO NOW/DAY%2B1DAY] AND -status_s:COMPLETE AND -status_s:DELETE AND -status_s:CLOSED"
      },
      {
        "desc": "All Tasks Due Today for Me",
        "name": "my-tasks-due-today",
        "value": "fq=due_tdt:[NOW/DAY TO NOW/DAY%2B1DAY] AND -status_s:COMPLETE AND -status_s:DELETE AND -status_s:CLOSED AND assignee_s:${user}"
      },
      {
        "desc": "All Tasks Overdue",
        "name": "tasks-overdue",
        "value": "fq=due_tdt:[* TO NOW/DAY] AND -status_s:COMPLETE AND -status_s:DELETE AND -status_s:CLOSED"
      },
      {
        "desc": "All Tasks Overdue for Me",
        "name": "my-tasks-overdue",
        "value": "fq=due_tdt:[* TO NOW/DAY] AND -status_s:COMPLETE AND -status_s:DELETE AND -status_s:CLOSED AND assignee_s:${user}"
      },
      {
        "desc": "(No Filter)",
        "name": "",
        "value": "",
        "default": true
      }
    ],
    "sorters": [
      {
        "desc": "Sort Create Date Asc",
        "name": "sort-date-asc",
        "value": "create_tdt ASC",
        "default": true
      },
      {
        "desc": "Sort Create Date Desc",
        "name": "sort-date-desc",
        "value": "create_tdt DESC"
      },
      {
        "desc": "Sort Case Name Asc",
        "name": "sort-by-name-asc",
        "value": "name ASC"
      },
      {
        "desc": "Sort Case Name Desc",
        "name": "sort-by-name-desc",
        "value": "name DESC"
      },
      {
        "desc": "(No Sort)",
        "name": "",
        "value": ""
      }
    ],
    "nodeTypes": [
      {
        "type": "prev",
        "icon": "fa fa-arrow-up"
      },
      {
        "type": "next",
        "icon": "fa fa-arrow-down"
